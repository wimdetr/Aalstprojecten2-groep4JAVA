<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root stylesheets="@../css/archiefscherm.css" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox prefHeight="106.0" prefWidth="929.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Archief Analyses" textFill="#85beb8">
               <font>
                  <Font name="Likhan" size="32.0" />
               </font>
            </Label>
            <Separator prefWidth="200.0" />
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox alignment="TOP_RIGHT" spacing="10.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding></HBox>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <TableView fx:id="archiefTableView" prefHeight="470.0" prefWidth="929.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="62.0" text="AnalyseID" />
          <TableColumn fx:id="voornaamColumn" prefWidth="174.0" text="Voornaam" />
            <TableColumn fx:id="naamColumn" prefWidth="204.0" text="Naam " />
            <TableColumn fx:id="bedrijfColumn" minWidth="7.0" prefWidth="183.0" text="Bedrijf" />
            <TableColumn fx:id="departementColumn" prefWidth="154.0" text="Afdeling" />
            <TableColumn fx:id="datumColumn" prefWidth="158.0" text="Laatst gewijzigd" />
            <TableColumn fx:id="resultaatColumn" minWidth="0.0" prefWidth="96.0" style="-fx-text-fill: green;" text="Resultaat" />
        </columns>
      </TableView>
   </center>
</fx:root>
