<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Archief " textFill="#85beb8">
               <font>
                  <Font name="Likhan" size="32.0" />
               </font>
            </Label>
            <Separator prefWidth="200.0" />
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox BorderPane.alignment="CENTER" />
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <TreeTableView fx:id="treeTableView" prefHeight="600.0" prefWidth="700.0" BorderPane.alignment="CENTER">
        <columns>
          <TreeTableColumn fx:id="bedrijfColumn" prefWidth="228.0" text="Bedrijf" />
          <TreeTableColumn fx:id="datumColumn" prefWidth="138.0" text="Datum " />
            <TreeTableColumn fx:id="jobcoachColumn" prefWidth="144.0" text="Jobcoach" />
            <TreeTableColumn fx:id="resultaatColumn" prefWidth="188.0" text="Resultaat" />
        </columns>
      </TreeTableView>
   </center>
</fx:root>
